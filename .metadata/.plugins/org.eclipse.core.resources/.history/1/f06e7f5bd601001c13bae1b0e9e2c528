package com.volume106;

import java.io.BufferedReader;
import java.io.IOException;
import java.io.InputStreamReader;
import java.util.Scanner;
import java.util.StringTokenizer;
 class P10611 {
	
	 
	 
	public int indexSearch(int luchuHeight) {
		
		return 0;
	}
	
	public void began() throws NumberFormatException, IOException {
		BufferedReader br=new BufferedReader(new InputStreamReader(System.in));
		int n = Integer.parseInt(br.readLine());
		
		
		int[] ladyHeightList =new int[n];
		
		StringTokenizer st = new StringTokenizer(br.readLine());
		for(int i=0;i<n;i++) {
			ladyHeightList[i] = Integer.parseInt(st.nextToken());
		}
		
		int q = Integer.parseInt(br.readLine());
		
		
		int luchuHeightList[] = new int[q];
		
		st = new StringTokenizer(br.readLine());
		for(int i=0; i<q;i++) {
			luchuHeightList[i] = Integer.parseInt(st.nextToken());
		}
		
		for(int i=0;i<q;i++) {
//			int begIndex=0;
//			int lastIndex = n-1;
//			int midIndex = 
			
			String smallest="X", tallest="X";
			for(int j=0; j<n ;j++) {
				
				if(luchuHeightList[i] > ladyHeightList[j]) {
					smallest = String.valueOf(ladyHeightList[j]);
				}
				
				if(luchuHeightList[i] < ladyHeightList[j]) {
					tallest = String.valueOf(ladyHeightList[j]);
					break;
				}
				
				
			}
			
			System.out.println(smallest+" "+tallest);
			
			
		}
	}
	public static void main(String args[]) throws NumberFormatException, IOException {
		//Scanner sc = new Scanner(System.in);
		Main ob = new Main();
		
		
		
	}
	
	
}
